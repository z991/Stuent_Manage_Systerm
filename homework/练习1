import time
mytime=time.time()
print(mytime)

seconds=int(mytime)%60
hours=int(mytime)//3600


mins=int(mytime)%3600
mins=(mins-seconds)//60

days=hours//24
hours=hours%24

months=days//30
days=days%30

years=months//12
months=months%12
print("从1970年至今，总共过了",years,"年",months,"月",days,"天",hours,"小时",mins,"分钟",seconds,"秒")